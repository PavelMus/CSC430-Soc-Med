    "mongoose": "^5.0.11",
    "node-sass": "^4.7.2",
    "nodemon": "^1.17.2",
    "npm": "^5.8.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "react-router-redux": "^4.0.8"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2"
  }
